<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AdminPanel</servlet-name>
        <servlet-class>test.AdminPanel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newCheckLogin</servlet-name>
        <servlet-class>security.Checklogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductFunction</servlet-name>
        <servlet-class>test.ProductFunction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductServlet</servlet-name>
        <servlet-class>test.ProductServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadFile</servlet-name>
        <servlet-class>test.UploadFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fail</servlet-name>
        <servlet-class>test.fail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Fail</servlet-name>
        <servlet-class>test.Fail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminPanel</servlet-name>
        <url-pattern>/AdminPanel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newCheckLogin</servlet-name>
        <url-pattern>/newCheckLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductFunction</servlet-name>
        <url-pattern>/ProductFunction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductServlet</servlet-name>
        <url-pattern>/ProductServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadFile</servlet-name>
        <url-pattern>/UploadFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fail</servlet-name>
        <url-pattern>/fail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Fail</servlet-name>
        <url-pattern>/Fail</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
